{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch yacas",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/cyacas/yacas/yacas",
            "args": ["--rootdir", "${workspaceRoot}/scripts/"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
